<div class="container" id="order-confirmed">
   <div class="row ct-section_header ct-section_header--type2"></div>
   <div class="row trip-details">
      <!-- <div class="venue-details col-9" style="background: url(<%#= image_url('bar1.jpg') %>;"> -->
      <div class="venue-details col-9">
        <div class="container">
          <div class="progress-bar">
            <div class="loaded-order-confirmed"></div>
          </div>
        </div>
        <div style="margin:10px auto 0 auto;">
          <h2>Hold Tight</h2>
          <p> We are ordering your taxi! Get ready to see the <%= @venue.venue_type %> of your dreams!</p> </br>
        <div>
          <%= render 'map' %>
        </div>

        </div>
        <div>
         <div class=" star-rating">
          <%= render 'stars' %>
         </div>
         <div class="d-flex justify-content-between">
          <%= render 'pricing' %>
          <p style="margin-top: 10px;">Open until <%= @venue.hours %></p>
         </div>
        </div>
      </div>

      <%# <div class="btn share-button">Share with your friends</div> %>
   </div>

</div>
