<div class="container">
   <div class="row ct-section_header ct-section_header--type2"></div>
   <div class="row trip-details">
      <!-- <div class="venue-details col-9" style="background: url(<%#= image_url('bar1.jpg') %>;"> -->
      <div class="venue-details col-9">
        <div>
          <h1> Congrats! </h1>
          <p> Your driver is on the way his name is <%= @booking.driver_name %>. Get ready to see the <%= @venue.venue_type %> of your dreams!</p>
          <p>Your estimated arrival is: <%= @booking.eta %></p>
          <p>License Plate number is: <%= @booking.license_plate %></p>
        </div>
        <div>
         <div class=" star-rating">
          <%= render 'stars' %>
         </div>
         <div class="pricing-details">
            <%= render 'pricing' %>
         </div>
         <p class="hours">Open until <%= @venue.hours %></p>
        </div>
      </div>

      <button type="button" class="btn share-button" data-toggle="modal" data-target="#rating-modal">
         Rate this venue
      </button>
      <button type="button" class="btn share-button" data-toggle="modal" data-target="#booking-show-link-modal">
         Share venue with friends
      </button>

      <%# <div class="btn share-button">Share with your friends</div> %>
   </div>
</div>
