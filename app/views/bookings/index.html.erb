<div class="container">
    <h1><%= current_user.name %>'s Past Results:</h1>

  <div class="container">
    <div class="cards">
      <% current_user.bookings.each do |booking| %>
            <div class="card" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(<%= booking.venue.picture %>);">
            <div class="pic_text" style="text-align:center">
              <%=booking.venue.name  %>
            </div>
            <div class="pic_text_2" style="text-align:center">
              <%= booking.venue.venue_type %>
            </div>
             <%= link_to "Check out the details again", booking %>
      <% end %>
    </div>
  </div>






</div>
